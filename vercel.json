{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "headers": {
                "Content-Type": "text/html; charset=utf-8"
            },
            "dest": "src/index.js"
        }
    ]
}